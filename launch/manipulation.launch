<launch>

  
  
  <!-- launch the robot in gazebo -->

  <include file="$(find ur5e_egp50_moveit_config)/launch/gazebo.launch" />

  <include file="$(find ur5e_egp50_moveit_config)/launch/ur5e_egp50_moveit_planning_execution.launch">
	  <arg name="sim" value="true"/>
  </include>

 
 <include file="$(find ur5e_egp50_moveit_config)/launch/moveit_rviz.launch">
	  <arg name="config" value="true"/>
  </include>



  <node pkg="seher" name="goal_state" type="go_up.py" output="screen"></node>
    

  <include file="$(find ur_manipulation)/launch/seher_demo.launch">
	  <arg name="prompts" value="false"/>
  </include>

  
</launch>